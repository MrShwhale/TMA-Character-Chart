#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/insert-module-globals@7.2.1/node_modules/insert-module-globals/bin/node_modules:/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/insert-module-globals@7.2.1/node_modules/insert-module-globals/node_modules:/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/insert-module-globals@7.2.1/node_modules:/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/insert-module-globals@7.2.1/node_modules/insert-module-globals/bin/node_modules:/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/insert-module-globals@7.2.1/node_modules/insert-module-globals/node_modules:/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/insert-module-globals@7.2.1/node_modules:/home/will/Documents/Development/Javascript/TMACharacterVisualizer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../insert-module-globals/bin/cmd.js" "$@"
else
  exec node  "$basedir/../insert-module-globals/bin/cmd.js" "$@"
fi
